<h1>Ironhack Cinema</h1>
<h3>Click on the movie to check the showtimes!</h3>
{{#each movies}}
    <img src="{{this.image}}" alt="movie poster"/>
    <h4>{{this.title}}</h4>
    <p><a href="/movie/{{this._id}}">See more</a></p>
    {{!-- request query version --}}
    {{!-- <p><a href="movie?movieId={{this.id}}">See more</a></p> --}} 
{{/each}}